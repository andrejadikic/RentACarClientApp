                        <MenuItem fx:id="newModelMenu" mnemonicParsing="false" onAction="#newModelMenu" text="Model" />
                        <MenuItem fx:id="newVehicleMenu" mnemonicParsing="false" onAction="#newVehicleMenu" text="Vehicle" />
                        <MenuItem fx:id="newTypeMenu" mnemonicParsing="false" onAction="#newTypeMenu" text="Type" />
                     </items></Menu>
                  <Menu mnemonicParsing="false" text="Open">
                     <items>
                        <MenuItem fx:id="openCompanyMenu" mnemonicParsing="false" onAction="#openCompanyMenu" text="Company" />
                        <MenuItem fx:id="openCityMenu" mnemonicParsing="false" onAction="#openCityMenu" text="Cities" />
                        <MenuItem fx:id="openModelMenu" mnemonicParsing="false" onAction="#openModelMenu" text="Models" />
                        <MenuItem fx:id="openVehicleMenu" mnemonicParsing="false" onAction="#openVehicleMenu" text="Vehicles" />
                        <MenuItem fx:id="openTypeMenu" mnemonicParsing="false" onAction="#openTypeMenu" text="Types" />
                     </items>
                  </Menu>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save Asâ€¦" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem fx:id="profileMenu" mnemonicParsing="false" onAction="#profileMenu" text="Profile" />
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyApp" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Go to">
              <items>
                <MenuItem fx:id="openUserMenu" mnemonicParsing="false" onAction="#openUserMenu" text="User" />
                  <MenuItem fx:id="openAdminMenu" mnemonicParsing="false" onAction="#openAdminMenu"  text="Admin" />
              </items>
            </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" fx:id="mainPage" />
  </children>
</VBox>
